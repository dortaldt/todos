{
  "manifest_version": 2,
  "name": "My React Chrome Extension",
  "version": "1.0",
  "description": "A Chrome extension built with React",
  "browser_action": {
    "default_popup": "build/index.html",
    "default_icon": "logo192.png"
  },
  "icons": {
    "48": "logo192.png",
    "96": "logo512.png"
  },
  "permissions": ["activeTab"]
}
